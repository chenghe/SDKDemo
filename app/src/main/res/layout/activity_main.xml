<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zsmarter.hechengbin.sdkdemo.MainActivity">

    <LinearLayout
        android:layout_width="368dp"
        android:layout_height="495dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="8dp">

        <Button
            android:id="@+id/button"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="start" />

        <Button
            android:id="@+id/sdkbutton"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="sdkstart" />

        <Button
            android:id="@+id/start_audio"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="startAudio" />

        <Button
            android:id="@+id/stop_audio"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="stopAudio" />

        <Button
            android:id="@+id/play_audio"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="playAudio" />

        <Button
            android:id="@+id/start_video"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="startVideo" />

        <Button
            android:id="@+id/start_warterMarker"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="wartermarker" />


        <Button
        android:id="@+id/start_cgnb"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:text="startcgnb" />
        <Button
            android:id="@+id/start_security"
            android:layout_width="100dp"
            android:layout_height="50dp"
            android:text="startsecurity" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>
